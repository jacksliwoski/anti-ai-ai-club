{
  "name": "anti-ai-ai-club",
  "version": "1.0.0",
  "description": "Audio protection platform to prevent unauthorized AI training on artists' music",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "audio",
    "protection",
    "ai",
    "metadata",
    "music",
    "watermark"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "music-metadata": "^9.0.0",
    "node-id3": "^0.2.6",
    "flac-metadata": "^1.0.4",
    "cors": "^2.8.5",
    "fluent-ffmpeg": "^2.1.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
